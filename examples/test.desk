shape Person {
    type name [x 30]u8
    name name origin
    age u16 name:x
}

people [z 1000]Person origin

proc add_person(name: Person.name, age: u16) {
    push people {
        name = name
        age = age
    }
}

proc avg(nums: []f64, to: f64) {
    sum f64 = 0
    for num in nums {
        sum += num
    }
    to = sum / len(nums)
}